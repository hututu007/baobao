<view catch:tap="onTap" class="container">
  <view class="head">
    <v-tag text="{{typeText}}" tag-class="tag" />
    <v-like class="like" read-only="{{true}}" like="{{true}}" count="{{classic.like_nums}}" />
  </view>
  <image class="{{classic.type==2?'music-img':'other-img'}}" src="{{classic.img}}"></image>
  <view class="text">{{classic.content}}</view>
</view>
